<script src="<%- config.root %>js/jquery.min.js"></script>
<script src="<%- config.root %>js/jquery.imagesloaded.min.js"></script>
<script src="<%- config.root %>js/gallery.js"></script>
<script src="<%- config.root %>js/main.js"></script>
<script>
  var duoshuoQuery = {short_name:"<%= theme.duoshuo_shortname %>"};

  (function() {
    var ds = document.createElement('script');
    ds.type = 'text/javascript';ds.async = true;
    ds.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') + '//static.duoshuo.com/embed.js';
    ds.charset = 'UTF-8';
    (document.getElementsByTagName('head')[0]
    || document.getElementsByTagName('body')[0]).appendChild(ds);
  })();

  $(function(){
    var tag_color = "<%= theme.tag_color %>".split(",");

    $.each($('.tagcloud a'),function(){
      var get_random = parseInt(Math.random()*10)
      $(this).css({
        'backgroundColor':tag_color[get_random]
      })
    })

    whblack.init("<%= theme.github_user_name %>")
  })
</script>


