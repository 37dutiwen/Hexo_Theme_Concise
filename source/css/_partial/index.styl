
//content
.content
  width page-width
  margin 0 auto
  .wrap
    width 730px
  .post-block
    margin-top default-distance
    padding:20px
  .post-time
    margin-top 10px
    a
      color #999999
      font-size 14px
  .post-entry
    margin-top 10px
    line-height 23px
    text-indent 32px
    padding-bottom 10px
    border-bottom 1px solid #F3F3F3
    .read-more
      text-align right
      margin-top 5px
      a
        color #9B9FAA